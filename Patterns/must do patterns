void print1(int n){
  for(int i = 0; i < n; i++)
  {
    for(int j = 0; j < n; j++)
    {
      std::cout << "* ";
    }
    std::cout << '\n';
  }
}
void print2(int n)
{
  for (int i = 0; i < n; i++)
  {
    for (int j = 0; j <= i; j++)
    {
      std::cout << "* ";
    }
    std::cout << '\n';
  }
}
void print3(int n){
  for(int i=1;i<=n;i++){
    for (int j=1;j<=i; j++){
      std::cout << j << " ";
    }
    std::cout << '\n';
  }
}
void print4(int n){
  for(int i=1;i<=n;i++){
    for (int j=1;j<=i; j++){
      std::cout << i << " ";
    }
    std::cout << '\n';
  }
}
void print5(int n){
  for(int i=1;i<=n;i++){
    for (int j=0;j<n-i+1; j++){
      std::cout << "* ";
    }
    std::cout << '\n';
  }
}
void print6(int n){
  for(int i=1;i<=n;i++){
    for (int j=1;j<=n-i+1; j++){
      std::cout << j << " ";
    }
    std::cout << '\n';
  }
}
void print7(int n){
  for(int i=0;i<n;i++){
    //space
    for(int j=0;j<n-i+1;j++){
      std::cout << " ";
    }
    //star
    for(int j=0;j<2*i+1;j++){
      std::cout << "*";
    }
    //space
    for(int j=0;j<n-i+1;j++){
      std::cout << " ";
    }
    std::cout << '\n';
  }
}
void print8(int n){
  for(int i=0;i<n;i++){
    //space
    for(int j=0;j<i;j++){
      std::cout << " ";
    }
    //star
    for(int j=0;j<2*n-(2*i+1);j++){
      std::cout << "*";
    }
    //space
    for(int j=0;j<i;j++){
      std::cout << " ";
    }
    std::cout << '\n';
  }
}
void print10(int n){
  for(int i=1;i<=2*n-1;i++){
    int stars = i;
    if(i > n) stars = 2*n-i;
    for(int j=1;j<=stars;j++){
      std::cout << "*";
    }
    std::cout << '\n';
  }
}
void print11(int n){
  int start = 1;
  for(int i=0;i<n;i++){
    if(i % 2 == 0) start = 1;
    else start = 0;
    for (int j = 0;j<=i;j++){
      std::cout << start << " ";
      start = 1- start; //will flip 1 to 0 and 0 to 1
    }
    std::cout << '\n';
  }
}
void print12(int n){
  int space = 2*(n-1);
  for (int i=1;i<=n;i++){
    //numbers
    for (int j=1;j<=i;j++){
      std::cout << j;
    }

    //spaces
    for(int j=1;j<=space;j++){
      std::cout << " ";
    }
    //numbers
    for(int j=i;j>=1;j--){
      std::cout << j;
    }
    std::cout << '\n';
    space -= 2;
  }
}
void print13(int n){
  int num = 1;
  for(int i=1;i<=n;i++){
    for(int j =1;j<=i;j++){
      std::cout << num << " ";
      num += 1;
    }
    std::cout << '\n';
  }
}
void print14(int n){
  for(int i=0;i<n;i++){
  for(char ch = 'A';ch <= 'A'+ i;ch++){
    std::cout << ch << " ";
  }
  std::cout << '\n';
  }
}
void print15(int n){
  for(int i=0;i<n;i++){
    for(char ch ='A'; ch <= 'A'+(n - i - 1); ch++){
    std::cout << ch << " ";
  }
  std::cout << '\n';
  }
}
void print16(int n){
  for(int i=0;i<n;i++){
    char ch = 'A' + i;
    for(int j=0;j<=i;j++){
      std::cout << ch << " ";
    }
    std::cout << '\n';
  }
}
void print17(int n){
  for(int i=0;i<n;i++){
    //space
    for(int j=0;j<n-i-1;j++){
      std::cout << " ";
    }
    //characters
    char ch = 'A';
    int breakpoint = (2*i+1) / 2;
    for(int j = 1;j<=2*i+1;j++){
      std::cout << ch << " ";
      if(j <= breakpoint)ch++;
      else ch--;
    }


    //space
    for(int j=0;j<n-i-1;j++){
      std::cout << " ";
    }
    std::cout << '\n';
  }
}
void print18(int n){
  for(int i=0;i<n;i++){
    for(char ch = 'E' - i;ch <= 'E'; ch++){
      std::cout << ch << " ";
    }
    std::cout << '\n';
  }
}
void print19(int n){
  //upper pattern of the symm
  int iniS = 0;
  for(int i=0;i<n;i++){
    //stars
    for(int j=1;j<=n-i;j++){
      std::cout << "*";
    }

    //spaces
    for(int j=0;j<iniS;j++){
      std::cout << " ";
    }


    //stars
    for(int j=1;j<=n-i;j++){
      std::cout << "*";
    }
    iniS += 2;
    std::cout << '\n';
  }

  iniS = 2*n-2;
  //lower pattern of the symm
  for(int i=1;i<=n;i++){
    //stars
    for(int j=1;j<=i;j++){
      std::cout << "*";
    }

    //spaces
    for(int j=0;j<iniS;j++){
      std::cout << " ";
    }


    //stars
    for(int j=1;j<=i;j++){
      std::cout << "*";
    }
    iniS -= 2;
    std::cout << '\n';
  }
}
void print20(int n){
  int spaces = 2*n-2;
  for(int i =1;i<=2*n-1;i++){
    int stars = i;
    if(i > n) stars = 2*n-i;
    //stars
    for(int j = 1;j<=stars;j++){
      std::cout << "*";
    }
    //spaces
    for(int j =1;j<=spaces;j++){
      std::cout << " ";
    }
    //stars
     for(int j = 1;j<=stars;j++){
      std::cout << "*";
    }
    std::cout << '\n';
    if(i < n) spaces -= 2;
    else spaces += 2;
  }
}
void print21(int n){
  for(int i = 0;i<n;i++){
    for(int j = 0;j < n;j++){
      if(i == 0 || j==0 || i==n-1 || j==n-1){
        std::cout << "*";
      }
      else std::cout << " ";
    }
    std::cout << '\n';
  }
}
void print22(int n){
  for(int i = 0;i<2*n-1;i++){
    for(int j= 0; j<2*n-1;j++){
      int top = i;
      int left = j;
      int right = (2*n-2)-j;
      int down = (2*n-2)-i;
      std::cout << (n-std::min(std::min(top,down), std::min(left,right)));
    }
    std::cout << '\n';
  }
}

int main(){
  int t;
  std::cin >> t;
  for(int i=0;i < t;i++){
    int n;
    std::cin >> n;
    print8(n);  
  }
  return 0;
}
